//create our add window
var addWin = Ti.UI.createWindow({
	backgroundColor : "#fff"
});

//create our title view
var addTitleLbl = Ti.UI.createLabel({
	top : "20dp",
	height : "50dp",
	left : "0dp",
	right : "0dp",
	text : "Add New",
	textAlign : "center",
	font : {fontSize : "30dp", fontWeight : "bold"},
	color : "black",
	backgroundColor : "#fff"
});

//add our parts to the window
addWin.add(addTitleLbl);
